{
  "description": "This demonstrates all of the OpenRTB extensions supported by Prebid Server. Very few requests will need all of these at once.",
  "config": {
    "mockBidders": [
      {
        "bidderName": "appnexus",
        "currency": "USD",
        "price": 1.00
      },
      {
        "bidderName": "rubicon",
        "currency": "USD",
        "price": 1.00
      }
    ]
  },
  "mockBidRequest": {
    "id": "some-request-id",
    "site": {
      "page": "prebid.org",
      "content": {
        "episode": 6,
        "title": "episodeName",
        "series": "TvName",
        "season": "season3",
        "len": 900
      }
    },
    "user": {
      "ext": {
        "consent": "gdpr-consent-string",
        "prebid": {
          "buyeruids": {
            "appnexus": "override-appnexus-id-in-cookie"
          }
        }
      }
    },
    "regs": {
      "ext": {
        "gdpr": 1,
        "us_privacy": "1NYN"
      }
    },
    "imp": [
      {
        "id": "some-impression-id",
        "video": {
          "mimes": [
            "video/mp4"
          ],
          "maxduration": 30,
          "protocols": [
            2,
            3
          ],
          "w": 640,
          "h": 480
        },
        "ext": {
          "appnexus": {
            "placementId": 12883452
          },
          "rubicon": {
            "accountId": 1001,
            "siteId": 113932,
            "zoneId": 535510
          }
        }
      }
    ],
    "tmax": 500,
    "ext": {
      "prebid": {
        "aliases": {
          "districtm": "appnexus"
        },
        "bidadjustmentfactors": {
          "appnexus": 1.01,
          "districtm": 0.98,
          "rubicon": 0.99
        },
        "cache": {
          "bids": {}
        },
        "channel": {
          "name": "video",
          "version": "1.0"
        },
        "adservertargeting": [
          {
            "key": "adt_hb_req_imp_maxduration",
            "source": "bidrequest",
            "value": "imp.video.mimes"
          },
          {
            "key": "adt_hb_req_imp_w",
            "source": "bidrequest",
            "value": "imp.video"
          },
          {
            "key": "adt_hb_req_site_contest_param",
            "source": "bidrequest",
            "value": "site.content"
          },
          {
            "key": "adt_{{BIDDER}}_impid",
            "source": "bidresponse",
            "value": "seatbid.bid.ext.key"
          },
          {
            "key": "adt_custom2",
            "source": "bidresponse",
            "value": "seatbid.bid.key"
          }
        ]
      }
    }
  },
  "expectedBidResponse": {
    "id": "some-request-id",
    "seatbid": [
      {
        "bid": [
          {
            "id": "appnexus-bid",
            "impid": "some-impression-id",
            "price": 1.01,
            "ext": {
              "origbidcpm": 1,
              "origbidcur": "USD",
              "prebid": {
                "meta": {
                  "adaptercode": "appnexus"
                },
                "type": "banner"
              }
            }
          }
        ],
        "seat": "appnexus"
      },
      {
        "bid": [
          {
            "id": "rubicon-bid",
            "impid": "some-impression-id",
            "price": 0.99,
            "ext": {
              "origbidcpm": 1,
              "origbidcur": "USD",
              "prebid": {
                "meta": {
                  "adaptercode": "rubicon"
                },
                "type": "banner"
              }
            }
          }
        ],
        "seat": "rubicon"
      }
    ],
    "bidid": "test bid id",
    "cur": "USD",
    "nbr": 0,
    "ext": {
      "warnings": {
        "general": [
          {
            "code": 10002,
            "message": "debug turned off for account"
          },
          {
            "code": 10007,
            "message": "incorrect value type for path: imp.video.mimes, value can only be string or number"
          },
          {
            "code": 10007,
            "message": "incorrect value type for path: imp.video, value can only be string or number"
          },
          {
            "code": 10007,
            "message": "incorrect value type for path: site.content, value can only be string or number"
          },
          {
            "code": 10007,
            "message": "value not found for path: ext.key for bidder"
          },
          {
            "code": 10007,
            "message": "value not found for path: key for bidder"
          },
          {
            "code": 10007,
            "message": "value not found for path: ext.key for bidder"
          },
          {
            "code": 10007,
            "message": "value not found for path: key for bidder"
          }
        ]
      }
    }
  },
  "expectedReturnCode": 200
}
