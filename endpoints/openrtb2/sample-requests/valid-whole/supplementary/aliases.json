      "hb_cache_id": "0",
      "hb_cache_id_unknown": "0",
      "hb_cache_path": "/pbcache/endpoint",
      "hb_cache_path_unknow": "/pbcache/endpoint",
      "hb_pb": "2.00",
      "hb_pb_unknown": "2.00"
    },
    "warnings": {
      "general": [
        {"code": 10002, "message": "debug turned off for account"}
      ]
    }
  },
  "expectedReturnCode": 200
