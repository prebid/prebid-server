{
  "description": "Amp request impression requests a bid from a bidder alias defined in root.ext.prebid.aliases",
  "config": {
    "mockBidders": [
      {"bidderName": "appnexus", "currency": "USD", "price": 2}
    ]
  },
  "mockBidRequest": {
    "id": "some-request-id",
    "site": {"page": "test.somepage.com"},
    "imp": [
      {
        "id": "my-imp-id",
        "banner": {
          "format": [
            {"w": 300, "h": 600}
          ]
        },
        "ext": {
          "unknown": {
            "placementId": 12883451
          }
        }
      }
    ],
    "ext": {
      "prebid": {
        "aliases": {
          "unknown": "appnexus"
        }
      }
    }
