{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Scalibur Adapter Params",
  "description": "A valid BidderParams object for the Scalibur adapter",
  "type": "object",
  "properties": {
    "placementId": {
      "type": "string",
      "minLength": 1
    },
    "bidfloor": {
      "type": "number",
      "minimum": 0
    },
    "bidfloorcur": {
      "type": "string"
    }
  },
  "required": [
    "placementId"
  ]
}
