{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "TrustX2 Adapter Params",
  "description": "A schema which validates params accepted by the TrustX2 adapter",
  "type": "object",
  "properties": {
    "publisher_id": {
      "type": "string",
      "description": "The publisher id"
    },
    "placement_id": {
      "type": "string",
      "description": "The placement id"
    }
  },
  "required": [
    "publisher_id",
    "placement_id"
  ]
}
