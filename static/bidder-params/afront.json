{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Afront Adapter Params",
  "description": "A schema which validates params accepted by the Afront adapter",
  "type": "object",
  "properties": {
    "clientAccountId": {
      "type": "string",
      "description": "Client account id",
      "minLength": 1
    },
    "dataSourceId": {
      "type": "string",
      "description": "Data source id",
      "minLength": 1
    }
  },
  "required": [
    "clientAccountId",
    "dataSourceId"
  ]
}