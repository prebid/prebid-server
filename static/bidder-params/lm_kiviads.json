{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Lm_Kiviads Adapter Params",
  "description": "A schema which validates params accepted by the lmkiviads adapter",
  "type": "object",
  "properties": {
    "env": {
      "type": "string",
      "description": "kivi environment",
      "minLength": 1
    },
    "pid": {
      "type": "string",
      "description": "Unique placement ID",
      "minLength": 1
    }
  },
  "required": [
    "env",
    "pid"
  ]
}
