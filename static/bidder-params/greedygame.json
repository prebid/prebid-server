{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "GreedyGame Adapter Params",
  "description": "A schema which validates params accepted by the GreedyGame adapter",
  "type": "object",

  "properties": {
    "publisherId": {
      "type": ["integer", "string"],
      "description": "Publisher ID",
      "minimum": 1,
      "pattern": "^[1-9][0-9]*$"
    }
  },

  "required": ["publisherId"]
}
